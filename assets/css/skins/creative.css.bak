.primary-footer .form-group input[type="email"] {
    color: #999;
    background-color: #fff;
    width: 76%;
    float: left;
    border: 0 !important;
}

.primary-footer .form-group button {
    background: #00aeef;
    border: 1px solid transparent;
    color: #fff;
}

