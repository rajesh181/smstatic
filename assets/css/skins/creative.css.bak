   color: #c5a4ff;
}
.primary-footer.brand-bg .form-group input::-moz-placeholder {
   color: #c5a4ff;  
}
.primary-footer.brand-bg .form-group input:-ms-input-placeholder {  
   color: #c5a4ff; 
}
 
